{
  "text": "",
  "formats": [
    {
      "screen": {
        "record format name": "myScreen",
        "external css": "public/css/style.css",
        "external javascript": "public/scripts/idlewordplay.js",
        "external javascript 2": "public/scripts/scripts.js"
      },
      "items": [
        {
          "id": "Layout1",
          "field type": "layout",
          "template": "responsive layout",
          "height": "100%",
          "width": "100%",
          "layout items": "5",
          "style rules": "@media screen { #_id_ > .puiresp { display:grid; grid-template-rows:50px 1fr; grid-template-columns:1fr repeat(3, 50px); padding-top:32px; padding-right:32px; padding-bottom:32px; padding-left:32px; box-sizing:border-box; } #_id_ > .puiresp > div:nth-of-type(5) { grid-column-end:span 4; } #_id_ > .puiresp > div { box-sizing:border-box; } #_id_ > .puiresp > div:nth-of-type(1) { overflow-x:visible; overflow-y:visible; } } ",
          "use viewport": "true",
          "max width": "720px",
          "center horizontally": "true",
          "max height": "965px",
          "center vertically": "true"
        },
        {
          "id": "iwp_reset",
          "field type": "icon",
          "css class": "iwp--icon",
          "left": "5px",
          "top": "5px",
          "icon": "material:restore",
          "height": "40px",
          "width": "40px",
          "css class 2": "iwp--icon--restart",
          "onclick": "iwp__onReset();",
          "layout": "Layout1",
          "container": "4"
        },
        {
          "id": "iwp_play",
          "field type": "icon",
          "css class": "iwp--icon",
          "icon": "material:play_circle_outline",
          "left": "5px",
          "top": "5px",
          "height": "40px",
          "width": "40px",
          "css class 2": "iwp--icon--play",
          "onclick": "iwp__onPlay();",
          "layout": "Layout1",
          "container": "2"
        },
        {
          "id": "iwp_pause",
          "field type": "icon",
          "css class": "iwp--icon",
          "icon": "material:pause_circle_outline",
          "left": "5px",
          "top": "5px",
          "height": "40px",
          "width": "40px",
          "css class 2": "iwp--icon--pause",
          "onclick": "iwp__onPause();",
          "layout": "Layout1",
          "container": "3"
        },
        {
          "id": "Icon1_copy3",
          "field type": "icon",
          "icon": "material:face",
          "left": "65px",
          "top": "10px",
          "layout": "Layout1",
          "container": "4"
        },
        {
          "id": "_name",
          "field type": "output field",
          "css class": "iwp--title",
          "value": "Idle Wordplay",
          "height": "100%",
          "layout": "Layout1",
          "container": "1"
        },
        {
          "id": "time",
          "field type": "html container",
          "left": "10px",
          "top": "10px",
          "white space": "normal",
          "width": "340px",
          "height": "75px",
          "color": "white",
          "layout": "Layout1",
          "container": "5"
        }
      ]
    }
  ],
  "keywords": [],
  "long name aliases": true
}